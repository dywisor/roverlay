[CRAN]
type             = rsync
rsync_uri        = cran.r-project.org::CRAN/src/contrib
src_uri          = http://cran.r-project.org/src/contrib
extra_rsync_opts = --exclude=/PACKAGES --exclude=/PACKAGES.gz --progress

